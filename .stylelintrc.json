{
  "extends": ["stylelint-config-sass-guidelines", "stylelint-config-rational-order", "stylelint-config-css-modules"],
  "rules": {
    "scss/at-extend-no-missing-placeholder": null,
    "order/properties-alphabetical-order": null,
    "scss/at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["value"]
      }
    ]
  }
}
